10 CLS:PRINT "BINOMIAL  (N x) p^x*(1-p)^(N-x)";:PRINT:S=0
20 LOCATE 0,0:PRINT :INPUT "N=",N
25 GOSUB 60:INPUT "p=",p:IF p>1 OR p<0 THEN 25
30 GOSUB 60:INPUT "x=",x:GOSUB 60
40 P=(FACT N/(FACT x*FACT(N-x))*p^x*(1-p)^(N-x):S=S+P
50 LOCATE 0,0:PRINT :PRINT "P(x)=";P;"S=";S:GOTO 30
60 LOCATE 0,0:PRINT :PRINT "                                ";:LOCATE 0,0:PRINT :RETURN
100 INPUT "DT:",DT:INPUT "MEDIA:",MED
110 INPUT "X:",X:Y=(1/DT/SQR(2*PI)*EXP(-.5*(X-MED)^2/DT^2):PRINT "Y=";Y;" ";Y*100;:GOTO 110

